const setInWindow = require('setInWindow');
const injectScript = require('injectScript');
const log = require('logToConsole');
const makeNumber = require('makeNumber');

// =============================================================================
// DEBUG
// =============================================================================

function debugLog(msg, obj) {
  if (data.enableDebug) {
    log('[EqualWeb]', msg, obj !== undefined ? obj : '');
  }
}

debugLog('Iniciando tag EqualWeb...');

// =============================================================================
// CONFIGURAÇÃO DO WIDGET
// =============================================================================

var iconType = makeNumber(data.iconType) || 1;

var interdealConfig = {
  sitekey: data.siteKey,
  domains: {
    js: 'https://cdn.equalweb.com/',
    acc: 'https://access.equalweb.com/'
  },
  Position: data.position,
  Menulang: data.menuLang,
  draggable: data.draggable,
  btnStyle: {
    vPosition: [data.vPosition, data.vPosition],
    margin: ['0', '0'],
    scale: [data.scale, data.scale],
    color: {
      main: data.mainColor,
      second: data.secondColor
    },
    icon: {
      outline: data.iconOutline,
      outlineColor: data.outlineColor,
      type: iconType,
      shape: data.iconShape
    }
  }
};

setInWindow('interdeal', interdealConfig, true);
debugLog('Widget config:', interdealConfig);

// =============================================================================
// CONFIGURAÇÃO DO TRACKING
// =============================================================================

if (data.enableTracking) {
  var trackingConfig = {
    eventName: data.trackingEventName || 'accessibility_interaction',
    trackProfiles: data.trackProfiles !== false,
    trackFeatures: data.trackFeatures !== false,
    trackMenuActions: data.trackMenuActions !== false,
    trackButtonClick: data.trackButtonClick !== false,
    trackLanguageChange: data.trackLanguageChange !== false,
    trackWidgetLoad: data.trackWidgetLoad !== false,
    debug: data.enableDebug
  };
  
  setInWindow('__ewTrackingConfig', trackingConfig, true);
  debugLog('Tracking config:', trackingConfig);
}

// =============================================================================
// URLs DOS SCRIPTS
// =============================================================================

var equalwebUrl = 'https://cdn.equalweb.com/core/5.2.0/accessibility.js';

var trackingUrl = 'https://gtm-templates.s3.amazonaws.com/equal-web.js';

// =============================================================================
// CARREGAMENTO DOS SCRIPTS
// =============================================================================

function onEqualWebLoaded() {
  debugLog('EqualWeb carregado com sucesso');
  
  if (data.enableTracking) {
    injectScript(trackingUrl, function() {
      debugLog('Script de tracking carregado');
      data.gtmOnSuccess();
    }, function() {
      debugLog('Falha no tracking, mas widget OK');
      data.gtmOnSuccess();
    }, 'ewTracking');
  } else {
    data.gtmOnSuccess();
  }
}

function onEqualWebFailed() {
  debugLog('Falha ao carregar EqualWeb');
  data.gtmOnFailure();
}

injectScript(equalwebUrl, onEqualWebLoaded, onEqualWebFailed, 'equalwebAccessibility');


