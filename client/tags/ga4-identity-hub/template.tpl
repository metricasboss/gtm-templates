___SANDBOXED_JS_FOR_WEB_TEMPLATE___

// Importar APIs necessárias
const gtag = require('createArgumentsQueue')('gtag', 'dataLayer');
const dataLayerPush = require('createQueue')('dataLayer');
const logToConsole = require('logToConsole');

// Obter o ID de medição configurado
const measurementId = data.measurementId || '';

// Verificar se o ID de medição foi configurado
if (!measurementId) {
  logToConsole('Erro: ID do Fluxo de Dados GA4 não especificado');
  data.gtmOnFailure();
  return;
}

// Objeto para armazenar os identificadores
let identifiers = {};
let pendingIds = 2; // Contador para controlar quando ambos os IDs foram obtidos

// Função para verificar se todos os IDs foram obtidos
function checkCompletion() {
  pendingIds--;
  if (pendingIds === 0) {
    // Enviar evento com os identificadores
    dataLayerPush({
      event: 'identity',
      client_id: identifiers.client_id || '',
      session_id: identifiers.session_id || ''
    });
    
    logToConsole('Identificadores GA4 obtidos:', identifiers);
    data.gtmOnSuccess();
  }
}

// Obter client_id
gtag('get', measurementId, 'client_id', function(value) {
  identifiers.client_id = value || '';
  checkCompletion();
});

// Obter session_id
gtag('get', measurementId, 'session_id', function(value) {
  identifiers.session_id = value || '';
  checkCompletion();
});


