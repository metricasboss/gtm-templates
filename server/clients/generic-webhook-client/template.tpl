// Importar funções necessárias
const claimRequest = require('claimRequest');
const getRequestPath = require('getRequestPath');
const getRequestBody = require('getRequestBody');
const getRequestHeader = require('getRequestHeader');
const runContainer = require('runContainer');
const addEventCallback = require('addEventCallback');
const JSON = require('JSON');
const returnResponse = require('returnResponse');
const setResponseStatus = require('setResponseStatus');
const setResponseHeader = require('setResponseHeader');
const logToConsole = require('logToConsole');
const getTimestampMillis = require('getTimestampMillis');

// Função isolada para debug log
function debugLog(message, obj) {
    if (data.enableDebug) {
        if (obj) {
            logToConsole('[Generic Webhook Client]', message, obj);
        } else {
            logToConsole('[Generic Webhook Client]', message);
        }
    }
}

// Configurações do webhook
const WEBHOOK_PATH = data.webhookPath || '/webhook';
const WEBHOOK_SECRET = data.webhookSecret;

// Verificar se deve processar esta requisição
if (getRequestPath() === WEBHOOK_PATH) {
    claimRequest();
    
    // Validação de segurança (opcional)
    if (data.enableSecurity && WEBHOOK_SECRET) {
        const authHeader = getRequestHeader('authorization') || 
                         getRequestHeader('x-webhook-secret') ||
                         getRequestHeader('x-api-key');
        
        if (!authHeader || authHeader !== WEBHOOK_SECRET) {
            debugLog('Unauthorized access attempt');
            setResponseStatus(401);
            setResponseHeader('error', 'Unauthorized');
            returnResponse();
            return;
        }
    }
    
    // Processar o webhook
    const requestBody = getRequestBody();
    
    if (!requestBody) {
        debugLog('Empty request body received');
        setResponseStatus(400);
        setResponseHeader('error', 'Empty request body');
        returnResponse();
        return;
    }
    
    // Tentar fazer parse do JSON
    const webhookData = JSON.parse(requestBody);
    
    if (!webhookData) {
        debugLog('Failed to parse JSON');
        setResponseStatus(400);
        setResponseHeader('error', 'Invalid JSON');
        returnResponse();
        return;
    }
    
    debugLog('Webhook received:', {
        path: getRequestPath(),
        timestamp: getTimestampMillis(),
        body_size: requestBody.length
    });
    
    // Criar evento simples com payload completo + metadata mínima
    const webhookEvent = {
        event_name: 'webhook_received',
        webhook_timestamp: getTimestampMillis(),
        webhook_path: getRequestPath(),
        webhook_data: webhookData
    };
    
    // Enviar para o GTM - deixar TODO o processamento para lá
    const startTime = getTimestampMillis();
    
    runContainer(webhookEvent, (bindToEvent) => {
        bindToEvent(addEventCallback)((containerId, eventData) => {
            const processingTime = getTimestampMillis() - startTime;
            debugLog('Webhook event processed:', {
                container: containerId,
                processing_time_ms: processingTime
            });
        });
    });
    
    // Resposta de sucesso
    setResponseStatus(200);
    setResponseHeader('message', 'Webhook processed successfully');
    returnResponse();
}


